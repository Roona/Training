<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.roona.hibernate.UserBO" table="m01_user">  
    <id name="userid" column="USERID" type="int">  
     <generator class="native"></generator>  
    </id>  
            
    <property name="username"  column="USERNAME" type="string"></property>
    <property name="phoneno"  column="PHONENO" type="string"></property>  
    <property name="applicationowner"  column="APPLICATIONOWNER" type="string"></property>   
    <property name="aocontactno"  column="AOCONTACTNO" type="string"></property>   
    
    
    
      
  </class>  
            
 </hibernate-mapping>  