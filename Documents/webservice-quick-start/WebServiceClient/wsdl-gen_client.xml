<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<project name="generate-client" default="main" basedir=".">
    <property name="java.home.sdk" 
        value="C:\Program Files\Java\jdk1.8.0_51" />
    <property name="wsdl.location" 
        value="http://localhost:8080/WS/HelloWorld?wsdl" />
    <target name="main">
        <exec executable="${java.home.sdk}\bin\wsimport.exe">
            <arg line="${wsdl.location} -s src -Xdebug -verbose -Xnocompile" />
        </exec>
    </target>
</project>

